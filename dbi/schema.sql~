DROP DATABASE IF EXISTS GAT;
CREATE DATABASE IF NOT EXISTS GAT;
USE GAT;
CREATE TABLE IF NOT EXISTS GA (id INT(10) PRIMARY KEY auto_increment, clus VARCHAR(100) NOT NULL, score FLOAT NOT NULL, tscore FLOAT NOT NULL);
CREATE TABLE IF NOT EXISTS SOLS (id INT(10) PRIMARY KEY auto_increment, fold INT(5) NOT NULL, sol VARCHAR(100) NOT NULL, score FLOAT NOT NULL, tscore FLOAT NOT NULL);
CREATE TABLE IF NOT EXISTS NODES (id INT(10) PRIMARY KEY auto_increment, node INT(10) NOT NULL UNIQUE, score FLOAT NOT NULL, visits INT(10) NOT NULL);
CREATE TABLE IF NOT EXISTS GENES (id INT(10) PRIMARY KEY auto_increment, name VARCHAR(50) NOT NULL UNIQUE, go VARCHAR(50), des longtext);
CREATE TABLE IF NOT EXISTS FOLDS (id INT(10) PRIMARY KEY auto_increment, fold INT(5) NOT NULL, gene INT(5));
